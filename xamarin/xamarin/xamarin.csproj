    <Reference Include="PassbookTicketGenerator">
      <HintPath>..\..\PassbookTicketGenerator\obj\Debug\PassbookTicketGenerator.dll</HintPath>
    </Reference>
    <Reference Include="ElectronicTicketGenerator">
